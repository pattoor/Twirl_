Class {
	#name : #Celda,
	#superclass : #Object,
	#instVars : [
		'alrededor',
		'lado'
	],
	#category : #Pinga
}

{ #category : #accessing }
Celda >> alrededor [

	^ alrededor
]

{ #category : #accessing }
Celda >> alrededor: anObject [

	alrededor := anObject
]

{ #category : #accessing }
Celda >> lado [

	^ lado
]

{ #category : #accessing }
Celda >> lado: anObject [

	lado := anObject
]
